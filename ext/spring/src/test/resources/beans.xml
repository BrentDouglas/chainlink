<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="classpath*:chainlink-ext-spring.xml"/>

    <bean id="runBatchlet" class="io.machinecode.chainlink.core.execution.artifact.batchlet.RunBatchlet" />
    <bean id="stopBatchlet" class="io.machinecode.chainlink.core.execution.artifact.batchlet.StopBatchlet" />
    <bean id="failBatchlet" class="io.machinecode.chainlink.core.execution.artifact.batchlet.FailBatchlet" />
    <bean id="errorBatchlet" class="io.machinecode.chainlink.core.execution.artifact.batchlet.ErrorBatchlet" />
    <bean id="injectedBatchlet" class="io.machinecode.chainlink.core.execution.artifact.batchlet.InjectedBatchlet" />
    <bean id="overrideBatchlet" class="io.machinecode.chainlink.core.execution.artifact.batchlet.OverrideBatchlet"/>
    <bean id="failStopBatchlet" class="io.machinecode.chainlink.core.execution.artifact.batchlet.FailStopBatchlet"/>
    <bean id="failProcessBatchlet" class="io.machinecode.chainlink.core.execution.artifact.batchlet.FailProcessBatchlet"/>
</beans>