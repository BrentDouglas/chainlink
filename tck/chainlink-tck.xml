<?xml version="1.0" encoding="UTF-8"?>
<chainlink xmlns="http://machinecode.io/xml/ns/chainlink"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://machinecode.io/xml/ns/chainlink http://machinecode.io/xml/ns/chainlink/chainlink_1_0.xsd">

    <job-operator name="default">
        <marshalling name="marshalling" ref="@marshalling.factory.class@"/>
        <mbean-server name="mbean-server" ref="@mbean.server.factory.class@"/>
        <artifact-loader name="artifact-loader" ref="@artifact.loader.factory.class@"/>
        <injector name="injector" ref="@injector.factory.class@"/>
        <execution-repository name="execution-repository" ref="@execution.repository.factory.class@"/>
        <registry name="registry" ref="@registry.factory.class@"/>
        <transport name="transport" ref="@transport.factory.class@"/>
        <executor name="executor" ref="@executor.factory.class@"/>

        <!--
        <class-loader name="class-loader" ref="@class.loader.factory.class@"/>
        <transaction-manager name="transaction-manager" ref="@transaction.manager.factory.class@"/>
        <job-loader name="job-loader" ref="@job.loader.factory.class@"/>
        <security name="security" ref="@security.factory.class@"/>

        <property name="io.machinecode.chainlink.transaction_manager.jndi_name" value="@transaction.manager.jndi.name@"/>
        -->
        <property name="io.machinecode.chainlink.executor.thread_factory.jndi_name" value="@thread.factory.jndi.name@"/>
        <property name="io.machinecode.chainlink.infinispan.cache_manager.jndi_name" value="@cache.manager.jndi.name@"/>
    </job-operator>

</chainlink>